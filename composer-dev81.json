{
  "name": "yivic/yivic-rest-api",
  "type": "wordpress-plugin",
  "description": "Yivic REST API - This WordPress plugin provides a secure and efficient way to handle REST API authentication and user access control",
  "version": "0.0.3",
  "time": "2026-01-08 21:29:36",
  "homepage": "https://yivic.com/wp-plugin-yivic-rest-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Phuc Nguyen",
      "email": "manhphucofficial@yahoo.com",
      "homepage": "https://yivic.com/members/phuc-nguyen",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0"
  },
  "require-dev": {
    "johnpbloch/wordpress-core": "^6.7.0",
    "wp-plugin/woocommerce": "*",
    "wpackagist-plugin/query-monitor": "^3.15",
    "wpackagist-theme/storefront": "^4.0",
    "wpackagist-theme/twentytwentyfour": "^1.0.0",
    "automattic/vipwpcs": "~3.0.0",
    "phpcompatibility/php-compatibility": "~9.3.5",
    "dealerdirect/phpcodesniffer-composer-installer": "~1.0.0",
    "phpstan/phpstan": "~2.0.1",
    "phpunit/phpunit": "~9.6",
    "mockery/mockery": "~1.6",
    "phpspec/prophecy": "~1.19.0",
    "php-stubs/wordpress-stubs": "~6.6.2",
    "brain/monkey": "~2.6.0",
    "10up/wp_mock": "~1.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wp-plugin/woocommerce",
        "version": "9.7.0",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/plugin/woocommerce.9.7.0.zip"
        }
      }
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "autoload": {
    "files": [
      "./yivic-rest-api-bootstrap.php"
    ],
    "psr-4": {
      "Yivic_Rest_Api\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
    ],
    "psr-4": {
      "Yivic_Rest_Api\\Tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "8.1.0"
    },
    "platform-check": false,
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "mcaskill/composer-exclude-files": true
    },
    "vendor-dir": "./vendor",
    "sort-packages": true
  },
  "extra": {
    "installer-paths": {
      "dev-docker/wordpress/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "dev-docker/wordpress/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ],
      "dev-docker/wordpress/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    },
    "mozart": {
      "dep_namespace": "Yivic_Rest_Api\\Deps\\",
      "dep_directory": "/src-deps/",
      "classmap_directory": "/classes/deps/",
      "classmap_prefix": "Yivic_Rest_Api",
      "packages": [
      ],
      "excluded_packages": [],
      "override_autoload": [],
      "delete_vendor_directories": false
    }
  },
  "scripts": {
    "post-install-cmd": [
      "cp -r `find ./vendor/johnpbloch/wordpress-core -maxdepth 1 -mindepth 1 | cut -c3- | grep -vE 'wp-content$|composer.json$|wp-cli.yml$|README.md$|LICENSE.md$'` ./dev-docker/wordpress",
      "rm -rf `find ./vendor/johnpbloch/wordpress-core -maxdepth 1 -mindepth 1 | cut -c3- | grep -vE 'license.txt|readme.html'`"
    ],
    "post-update-cmd": [
      "cp -r `find ./vendor/johnpbloch/wordpress-core -maxdepth 1 -mindepth 1 | cut -c3- | grep -vE 'wp-content$|composer.json$|wp-cli.yml$|README.md$|LICENSE.md$'` ./dev-docker/wordpress",
      "rm -rf `find ./vendor/johnpbloch/wordpress-core -maxdepth 1 -mindepth 1 | cut -c3- | grep -vE 'license.txt|readme.html'`"
    ],
    "dev-update": [
      "cp -r `find ./vendor/johnpbloch/wordpress-core -maxdepth 1 -mindepth 1 | cut -c3- | grep -vE 'wp-content$|composer.json$|wp-cli.yml$|README.md$|LICENSE.md$'` ./dev-docker/wordpress",
      "rm -rf `find ./vendor/johnpbloch/wordpress-core -maxdepth 1 -mindepth 1 | cut -c3- | grep -vE 'license.txt|readme.html'`"
    ],
    "phpcs": [
      "./vendor/bin/phpcs"
    ],
    "phpstan": [
      "./vendor/bin/phpstan"
    ],
    "phpunit": [
      "./vendor/bin/phpunit"
    ],
    "test": [
      "@phpcs",
      "@phpstan",
      "@phpunit"
    ]
  }
}
